<?xml version="1.0" encoding="utf-8"?>
<Containers>
  <TemplateURL>https://raw.githubusercontent.com/paulpoco/docker-templates/master/paulpoco/flexget-dev.xml</TemplateURL>
  <Beta>True</Beta>
  <Category>Downloaders:</Category>
  <Name>flexget</Name>
  <Requires>binhex-delugevpn</Requires>
  <Description>
    FlexGet is a multipurpose automation tool for all of your media
    Requires binhex-delugevpn to be installed 1st
    And do Q24 on the Binhex VPN FAQ
  </Description>
  <Overview>
  </Overview>
  <Support>http://lime-technology.com/forum/index.php?topic=48629.0</Support>
  <Registry>https://hub.docker.com/r/wiserain/flexget:latest</Registry>
  <GitHub>https://github.com/wiserain/docker-flexget</GitHub>
  <Repository>wiserain/flexget:latest</Repository>
  <Project>http://flexget.com/</Project>
  <BindTime>true</BindTime>
  <Privileged>true</Privileged>
  <Networking>
    <Mode>none</Mode>
  </Networking>
  <Environment>
    <Variable>
      <Name>FG_WEBUI_PASSWD</Name>
      <Value>password here 16 characters</Value>
    </Variable>    
    <Variable>
      <Name>UMASK</Name>
      <Value>000</Value>
    </Variable>
    <Variable>
      <Name>PUID</Name>
      <Value>99</Value>
    </Variable>
    <Variable>
      <Name>PGID</Name>
      <Value>100</Value>
    </Variable>
  </Environment>
  <Data>
    <Volume>
      <HostDir>/mnt/user/appdata/flexget</HostDir>
      <ContainerDir>/config</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/user/Media</HostDir>
      <ContainerDir>/data</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <WebUI></WebUI>
  <Icon>https://raw.githubusercontent.com/paulpoco/docker-templates/master/paulpoco/images/Deluge-Flexget-icon.png</Icon>
  <DonateText></DonateText>
  <DonateLink></DonateLink>
  <DonateImg></DonateImg>
  <ExtraParams>--net=container:binhex-delugevpn</ExtraParams>
</Containers>
